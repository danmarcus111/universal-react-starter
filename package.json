{
  "name": "full-test",
  "version": "1.0.0",
  "description": "trying first full test of isomorphic react",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:clean && npm run build:client && npm run build:server",
    "build:clean": "rm -rf dist/components",
    "build:client": "NODE_ENV=production webpack -p",
    "build:server": "babel src/server -d dist && babel src/client/components -d dist/components",
    "start:dev": "watch 'npm run build:server' src",
    "start:server": "node dist/server"
  },
  "author": "Dan Marcus",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-preset-env": "^1.1.8",
    "babel-preset-react": "^6.23.0",
    "css-loader": "^0.26.1",
    "style-loader": "^0.13.1",
    "watch": "^1.0.2",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.1"
  },
  "dependencies": {
    "body-parser": "^1.16.1",
    "cors": "^2.8.1",
    "ejs": "^2.5.6",
    "express": "^4.14.1",
    "morgan": "^1.8.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router-dom": "^4.0.0-beta.6"
  }
}
